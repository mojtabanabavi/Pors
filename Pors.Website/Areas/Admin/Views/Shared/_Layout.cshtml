<!DOCTYPE html>
<html lang="en">
<head>
    <title>پرس - @ViewData["Title"]</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0 shrink-to-fit=no">

    <!-- favicon -->
    <link rel="icon" type="image/png" href="./img/brand/favicon.png">

    <!-- librarys -->
    <link rel="stylesheet" href="./lib/bootstrap/css/bootstrap.min.css" />

    <!-- fonts -->
    <link rel="stylesheet" href="./fonts/yekan/css/yekan.css">
    <link rel="stylesheet" href="./fonts/nucleo/css/nucleo.css">
    <link rel="stylesheet" href="./fonts/fontawesome/css/all.min.css">

    <!-- styles -->
    <link rel="stylesheet" href="./css/argon.css" />
    <link rel="stylesheet" href="./css/main.css" />

</head>
<body>

    <!-- sidebar -->
    <partial name="_sidebar" />

    <!-- main content -->
    <div class="main-content">

        <!-- navbar -->
        <partial name="_navbar" />

        <!-- header -->
        <div class="header bg-gradient-primary pb-8 pt-5 pt-md-8">
            <div class="container-fluid">
                <div class="header-body">
                    @RenderSection(name: "header",required: false)
                </div>
            </div>
        </div>

        <!-- content -->
        <div class="container-fluid mt--7">
            @RenderBody()
        </div>
    </div>

    <!-- librarys -->
    <script src="./lib/jquery/js/jquery.min.js"></script>
    <script src="./lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="./lib/survey/js/survey.jquery.min.js"></script>

    <!-- scripts -->
    <script src="./js/argon.js"></script>
    <script src="./js/main.js"></script>
</body>
</html>